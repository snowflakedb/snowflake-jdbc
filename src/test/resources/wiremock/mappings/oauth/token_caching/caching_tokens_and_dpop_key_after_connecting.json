{
  "mappings": [
    {
      "scenarioName": "Successful writing access token and refresh token to empty cache",
      "requiredScenarioState": "Started",
      "newScenarioState": "Acquired tokens",
      "request": {
        "urlPathPattern": "/oauth/token-request.*",
        "method": "POST",
        "headers": {
          "Authorization": {
            "contains": "Basic"
          },
          "Content-Type": {
            "contains": "application/x-www-form-urlencoded; charset=UTF-8"
          },
          "DPoP": {
            "matches": ".*"
          }
        },
        "bodyPatterns": [
          {
            "contains": "grant_type=client_credentials&scope=session%3Arole%3AANALYST"
          }
        ]
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "access_token": "access-token-123",
          "refresh_token": "refresh-token-123",
          "token_type": "DPoP",
          "username": "user",
          "scope": "refresh_token session:role:ANALYST",
          "expires_in": 600,
          "refresh_token_expires_in": 86399,
          "idpInitiated": false
        }
      }
    },
