{
  "mappings": [
    {
      "request": {
        "url": "/latest/api/token",
        "method": "PUT",
        "headers": {
          "X-aws-ec2-metadata-token-ttl-seconds": {
            "equalTo": "21600"
          }
        }
      },
      "response": {
        "status": 200,
        "body": "mock-token-12345"
      }
    },
    {
      "request": {
        "url": "/latest/dynamic/instance-identity/document",
        "method": "GET",
        "headers": {
          "X-aws-ec2-metadata-token": {
            "equalTo": "mock-token-12345"
          }
        }
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "devpayProductCodes": null,
          "marketplaceProductCodes": null,
          "availabilityZone": "us-west-2a",
          "privateIp": "10.0.0.1",
          "version": "2017-09-30",
          "instanceId": "i-1234567890abcdef0",
          "billingProducts": null,
          "instanceType": "t2.micro",
          "accountId": "123456789012",
          "imageId": "ami-12345678",
          "pendingTime": "2023-01-01T00:00:00Z",
          "architecture": "x86_64",
          "kernelId": null,
          "ramdiskId": null,
          "region": "us-west-2"
        }
      }
    }
  ]
} 