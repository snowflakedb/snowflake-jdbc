{
  "mappings": [
    {
      "scenarioName": "Invalid scope authorization error",
      "request": {
        "urlPathPattern": "/oauth/authorize.*",
        "method": "GET"
      },
      "response": {
        "status": 200
      },
      "serveEventListeners": [
        {
          "name": "webhook",
          "parameters": {
            "method": "GET",
            "url": "http://localhost:8002/snowflake/oauth-redirect?error=invalid_scope&error_description=One+or+more+scopes+are+not+configured+for+the+authorization+server+resource."
          }
        }
      ]
    }
  ]
}